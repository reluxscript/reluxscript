import { useState } from '@minimact/core';
export default function Counter() {
  const [count, setCount] = useState(0);
  const [message, setMessage] = useState("Hello");
  return <div key="1" id="counter-root">
      <span key="1.1" id="counter-value">{count}</span>
      <span key="1.2" id="message">{message}</span>
      <button key="1.3" id="increment-btn" type="button" onClick={() => setCount(count + 1)}>
        Increment
      </button>
    </div>;
}